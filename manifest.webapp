{
  "name": "Explorer",
  "description": "Allows to select a file and share it with an app, bluetooth or email.",
  "type": "privileged",
  "role": "system",
  "launch_path": "/index.html",
  "version": "0.1.2",
  "icons": {
    "128": "/css/images/logo_128.png"
  },
  "developer": {
    "name": "Sebastian Rajo",
    "url": "http://www.prottotipo.com/explorer"
  },
  "permissions": {
    "desktop-notification": {
      "description": "To show notifications to the user"
    },
    "device-storage:sdcard": {
      "description": "To pick a file from the SDCARD",
      "access": "readonly"
    }
  },
"locales": {
	"es": {
  "name": "Explorador",
  "description": "Permite seleccionar un archivo y compartirlo con una aplicaci√≥n, bluetooth o email"
		},
  "activities": {
    "pick": {
      "disposition": "inline",
      "returnValue": true,
      "href": "/index.html"
    }
  },
  "default_locale": "en"
	}
}